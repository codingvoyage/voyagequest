getGlobal GAME_STATE --> gameState


if gameState < 65
    print "DERP"
    print gameState

    freezeInputs
    dialogBox "Sebastian Profile" "Uh... why would I go here? I'm busy now..."

    assumeControlOfPlayer
    wait 20

    -- WALK BACK DOWN
    setAnimationDirection 1
    setVelocity 0 0.25
    moveTileAmount 1
    setAnimationDirection 1

    unfreezeInputs
    endthread
endif


mapChange "src/res/SPECIALHOUSE.tmx" 730 1400

endthread